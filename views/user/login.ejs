<!DOCTYPE html>
<html lang="en">
  <%- include("../partials/head.ejs",{title:"User Login"}) %>
  <body>
    <div class="container" id="signup">
      <form action="/user/login" method="post">
        <div class="row user">
          <label class="col-md-4 offset-md-6 col-sm-3">USERNAME </label>
          <input
            class="col-md-4 offset-md-6 col-sm-1"
            type="text"
            name="username"
            required
          />
        </div>
        <div class="row user">
          <label class="col-md-4 offset-md-6 col-sm-3">PASSWORD</label>
          <input
            class="col-md-4 offset-md-6 col-sm-1"
            type="password"
            name="password"
            required
          />
        </div>

        <div class="col-md-4 offset-md-6 col-sm-1 user" id="newuserbutton">
          <button>Login</button>
        </div>
      </form>
    </div>
  </body>
</html>
